{
  "name": "codejam-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite --host",
    "test": "vitest run",
    "test:watch": "vitest",
    "codegen": "npx graphql-codegen --watch",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "dev-gen": "concurrently \"npx graphql-codegen --watch \" \"npx vite --host\"",
    "format": "prettier --write .",
    "lint": "eslint .",
    "format:lint": "pnpm run format && pnpm run lint",
    "format:ci": "prettier . --check"
  },
